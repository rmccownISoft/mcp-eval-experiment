{
  "name": "mcp-test-suite",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript test suite to evaluate GraphQL-API MCP server effectiveness for AI agents",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "mcp-test-suite": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.js",
      "types": "./dist/lib/*.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "config/**/*",
    "test-prompts/**/*",
    "examples/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/tests/**/*.test.js",
    "test:dev": "tsc && npm test",
    "test:env": "tsc && node dist/examples/basic-usage.js",
    "demo:mcp-client": "tsc && node dist/examples/mcp-client-demo.js",
    "demo:phase3": "tsc && node dist/examples/phase3-claude-demo.js",
    "demo:basic": "tsc && node dist/examples/basic-usage.js",
    "cli": "node dist/cli/index.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "graphql",
    "ai-testing",
    "llm-evaluation",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "commander": "^12.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
